<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 填充颜色 -->
    <solid android:color="#50000000" />
    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="1px"
        android:color="@android:color/transparent" />
    <!-- 矩形的圆角半径 -->
    <corners android:radius="10dp" />
    <padding
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp" />

</shape>